<script>
  const regions = [
    {% for district in districts_data %}
    {
      id: "{{ district.object_id }}",
      label_id: "{{ district.object_id }}-text",
      name: "{{ district.name }}",
      code: {{ district.id }},
      intensity: {{ district.intensity }},
      count: {{ district.count }}
    },
    {% endfor %}
  ];

  function goToRegion(element) {
    window.location.href = "/events/" + element.code;
  }

  window.onload = function () {
    const container = document.querySelector("#events-map");
    const paths = container.querySelectorAll("path");

    paths.forEach((path) => {
      let region = regions.find(item => item.id == path.id);
      path.classList.add("bg-" + region.intensity);

      path.onclick = (e) => goToRegion(region);

      path.addEventListener("mouseenter", function (e) {
        e.target.classList.add("active");
      });

      path.addEventListener("mouseleave", function (e) {
        e.target.classList.remove("active");
      });

      const label = container.querySelector("#" + path.id + "-text");

      label.onclick = (e) => goToRegion(regions.find(item => item.label_id == e.target.id));

      label.addEventListener("mouseenter", function (e) {
        const el = regions.find(item => item.label_id == e.target.id);
        container.querySelector("#" + el.id).classList.add("active");
      });

      label.addEventListener("mouseleave", function (e) {
        const el = regions.find(item => item.label_id == e.target.id);
        container.querySelector("#" + el.id).classList.remove("active");
      });
    });
  }
</script>
